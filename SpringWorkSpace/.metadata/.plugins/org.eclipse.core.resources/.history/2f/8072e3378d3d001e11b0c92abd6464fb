package in.mindcraft.SpringApp;

public class Student {
private String name;
private  int roll;
private String resd;


public Student() {
	super();
	name="Pralhad";
	roll=10;
	resd="Ohio";
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getId() {
	return roll;
}
public void setId(int roll) {
	this.roll = roll;
}
public String getResd() {
	return resd;
}
public void setResd(String resd) {
	this.resd = resd;
}




}
